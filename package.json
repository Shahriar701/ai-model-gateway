{
  "name": "ai-model-gateway",
  "version": "0.1.0",
  "description": "Production-ready AI Model Gateway with MCP integration for e-commerce platforms",
  "bin": {
    "ai-model-gateway": "bin/ai-model-gateway.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:dev": "ENVIRONMENT=dev cdk deploy --all",
    "deploy:prod": "ENVIRONMENT=prod cdk deploy --all",
    "destroy": "cdk destroy --all",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap",
    "setup:dev": "./scripts/setup-dev.sh",
    "clean": "rm -rf lib/ cdk.out/ coverage/ node_modules/.cache/",
    "typecheck": "tsc src/**/*.ts --noEmit --skipLibCheck",
    "test:unit": "jest --testPathPattern=test/.*\\.test\\.ts$",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:system": "ts-node test/integration/run-integration-tests.ts integration",
    "test:integration:mcp": "ts-node test/integration/run-integration-tests.ts mcp",
    "test:integration:comprehensive": "ts-node test/integration/run-integration-tests.ts system",
    "test:integration:all": "ts-node test/integration/run-integration-tests.ts all",
    "test:e2e": "jest --testPathPattern=test/.*\\.e2e\\.ts$",
    "test:smoke": "jest --testPathPattern=test/.*\\.smoke\\.ts$",
    "test:performance": "jest --testPathPattern=test/.*\\.performance\\.ts$",
    "test:performance:baseline": "ts-node scripts/performance-baseline.ts",
    "test:performance:regression": "ts-node scripts/performance-regression.ts",
    "dev": "npm run watch & npm run test:watch",
    "validate": "npm run typecheck && npm run lint && npm run test && npm run build",
    "validate:infrastructure": "cdk synth --validation",
    "validate:config": "ts-node scripts/validate-config.ts",
    "validate:feature-flags": "ts-node scripts/validate-feature-flags.ts",
    "validate:traffic-switch": "ts-node scripts/validate-traffic-switch.ts",
    "validate:rollback": "ts-node scripts/validate-rollback.ts",
    "deploy:blue-green": "ts-node scripts/deploy-blue-green.ts",
    "traffic:switch": "ts-node scripts/traffic-switch.ts",
    "wait:deployment-complete": "ts-node scripts/wait-deployment.ts",
    "health-check": "ts-node scripts/health-check.ts",
    "monitor:metrics": "ts-node scripts/monitor-metrics.ts",
    "cleanup:green-environment": "ts-node scripts/cleanup-environment.ts green",
    "cleanup:failed-deployment": "ts-node scripts/cleanup-environment.ts",
    "notify:rollback": "ts-node scripts/notify-rollback.ts",
    "detect:config-drift": "ts-node scripts/detect-config-drift.ts",
    "security:scan": "npm audit --audit-level moderate",
    "security:validate": "ts-node scripts/security-validate.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "aws-cdk": "2.1031.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-cloudwatch": "^3.922.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-eventbridge": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-ssm": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "aws-cdk-lib": "2.215.0",
    "aws-sdk": "^2.1691.0",
    "aws-xray-sdk-core": "^3.9.0",
    "axios": "^1.6.0",
    "constructs": "^10.0.0",
    "redis": "^4.6.0",
    "zod": "^3.23.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "ai",
    "llm",
    "mcp",
    "gateway",
    "platform-engineering",
    "infrastructure-as-code"
  ],
  "author": "Platform Engineering Team",
  "license": "MIT"
}
